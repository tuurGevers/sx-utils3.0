{
	"name": "sx-static",
	"version": "0.3.1",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./dist/*": "./dist/*"
	},
	"files": [
		"dist/**/*",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0",
		"@sveltejs/kit": "^1.20.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.0",
		"publint": "^0.1.9",
		"svelte": "^4.0.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"axios": "^1.4.0",
		"fs-extra": "^11.1.1",
		"node-html-parser": "^6.1.6",
		"svelte-preprocess": "^5.0.4"
	}
}
